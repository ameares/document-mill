\newcommand{\voltsummaryplot}[8]{
\begin{tikzpicture}[baseline]
\begin{axis}[
    small,
    width=0.25\textwidth,
    height=4.5cm,
    title={Vrms Comparison},
    %ylabel={Voltage [Vrms]},
    symbolic x coords={L1Inv, L2Inv, L1In, L2In},
    xtick=data,
    ymajorgrids=true,
    grid style=dashed,
    error bars/y dir=both,
    error bars/y explicit,
    ymin=118, ymax=122,
]

\addplot+[
    only marks,
    error bars/.cd,
        y dir=both,
        y explicit,
    ] coordinates {
    (L1Inv, #1) +- (0, #2)
    (L2Inv, #3) +- (0, #4)
    (L1In, #5) +- (0, #6)
    (L2In, #7) +- (0, #8)
};


\end{axis}
\end{tikzpicture}}

\newcommand{\currentsummaryplotcharge}[6]{
